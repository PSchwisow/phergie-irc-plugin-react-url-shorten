{
    "name": "pschwisow/phergie-irc-plugin-react-url-shorten",
    "description": "Phergie plugin to provide URL shortening services for Url plugin (and others)",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "license": "BSD-2-Clause",
    "support": {
        "email": "patrick.schwisow@gmail.com",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/PSchwisow/phergie-irc-plugin-react-url-shorten/issues",
        "source": "https://github.com/PSchwisow/phergie-irc-plugin-react-url-shorten"
    },
    "minimum-stability": "dev",
    "require": {
        "phergie/phergie-irc-bot-react": "~1",
        "wyrihaximus/phergie-url": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "phake/phake": "2.0.0-beta2",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PSchwisow\\Phergie\\Plugin\\UrlShorten\\": "src"
        }
    },
    "autoload-dev": {
